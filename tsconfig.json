{
  "compilerOptions": {
    "declaration": false,
    "esModuleInterop": true,
    "module": "Node16",
    "moduleResolution": "Node16",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "jsx": "preserve",
    "lib": ["esnext"],
    "baseUrl": ".",
    "outDir": "dist"
  },
  "exclude": [
    "node_modules"
  ]
}
